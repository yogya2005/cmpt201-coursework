#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  char *line = NULL;
  size_t len = 0;
  printf("Please enter your text:");
  getline(&line, &len, stdin);

  char *saveptr;
  char *token;
  printf("Tokens:\n");

  token = strtok_r(line, "bin/", &saveptr);

  while (token != NULL) {
    printf("%s\n", token);
    token = strtok_r(NULL, " ", &saveptr);
  }

  free(line);
  return 0;
}

#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  char *line = NULL;
  size_t len = 0;
  printf("Enter programs to run:");
  getline(&line, &len, stdin);

  char *saveptr;
  char *token;

  token = strtok_r(line, "/usr/bin/", &saveptr);
  if (token == NULL)
    printf("Exec failure");
  else
    printf("%s", token);

  free(line);
  return 0;
}
